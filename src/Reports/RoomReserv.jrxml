	<parameter name="fromMonth" class="java.lang.Integer"/>
	<parameter name="toMonth" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
            rr.`reservation_id` AS Reserv_id,
            rr.`customer_id` AS Customer_id,
            c.`name` AS customer_name,
            rr.`room_id` AS Room_id,
            rr.`reserve_Date` AS Reserve_Date,
            rr.`checkInDate` AS CheckInDate,
            rr.`checkOutDate` AS CheckOutDate,
            rr.`price` AS Price,
            rr.`status` AS Status
        FROM
            `roomreservation` rr
        JOIN
            `customers` c ON rr.`customer_id` = c.`customer_id`
        WHERE
            MONTH(rr.`reserve_Date`) BETWEEN $P{fromMonth} AND $P{toMonth}
        ORDER BY
            rr.`reservation_id`;]]>
	</queryString>
