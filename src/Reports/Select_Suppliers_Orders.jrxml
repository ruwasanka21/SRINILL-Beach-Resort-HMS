	<queryString>
		<![CDATA[SELECT
     suporders.`billNo` AS suporders_billNo,
     suporders.`itemName` AS suporders_itemName,
     suporders.`supName` AS suporders_supName,
     suporders.`qty` AS suporders_qty,
     suporders.`itemPrice` AS suporders_itemPrice,
     suporders.`Total` AS suporders_Total,
     suporders.`method` AS suporders_method,
     suporders.`Date` AS suporders_Date,
     suporders.`paidStatus` AS suporders_paidStatus,
     supplier.`supID` AS supplier_supID,
     supplier.`supName` AS supplier_supName,
     supplier.`supCategory` AS supplier_supCategory,
     supplier.`supAddress` AS supplier_supAddress,
     supplier.`supPhone` AS supplier_supPhone,
     supplier.`supEmail` AS supplier_supEmail,
     supplier.`supBank` AS supplier_supBank,
     supplier.`supBankAccNo` AS supplier_supBankAccNo,
     supplier.`AddedDate` AS supplier_AddedDate
FROM
     `suporders` suporders,
     `supplier` supplier
WHERE
     supplier.`supName` = $P{selectSup} AND
     suporders.`supName` = $P{selectSup}]]>
	</queryString>
