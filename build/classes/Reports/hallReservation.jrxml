	<queryString>
		<![CDATA[SELECT
                hr.`reservation_id` AS Reserv_id,
                hr.`customer_id` AS Customer_id,
                c.`name` AS customer_name,
                hr.`hall_id` AS Hall_id,
                hr.`reserve_Date` AS Reserve_Date,
                hr.`checkInDate` AS CheckInDate,
                hr.`price` AS Price,
                hr.`status` AS Status
            FROM
                `hallreservation` hr
            JOIN
                `customers` c ON hr.`customer_id` = c.`customer_id`
            WHERE
                MONTH(STR_TO_DATE(hr.`reserve_Date`, '%Y-%m-%d')) BETWEEN $P{fromMonth} AND $P{toMonth}
            ORDER BY hr.`reservation_id`]]>
	</queryString>
